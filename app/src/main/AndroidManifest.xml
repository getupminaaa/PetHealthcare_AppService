<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pethealthcare_appservice">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".sign.loginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="google.com"
                    android:scheme="http" />
            </intent-filter>
        </activity>
        <activity android:name=".sign.passwordResetActivity"></activity>
        <activity android:name=".sign.SignUpActivity"></activity>

        <activity android:name=".user.SetProfileActivity"></activity>
        <activity android:name=".user.updateProfileActivity"></activity>

        <activity android:name=".MainActivity"></activity>

        <activity android:name=".pet.Pet_MainActivity"></activity>
        <activity android:name=".pet.Pet_AddActivity"></activity>

        <activity android:name=".todoList.TodoList_pNameActivity"></activity>
        <activity android:name=".todoList.TodoList_MainActivity"></activity>
        <activity android:name=".todoList.Add_TodoActivity"></activity>

    </application>

</manifest>